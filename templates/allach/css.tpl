a {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: /*{$font.size.normal}*/px;
    font-style: normal;
    text-decoration: none;
    color: #333333;
  }

body, {
    background : url('/*{$style.images}*//steelbg.png');
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-decoration : none;
  }

p, td {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: /*{$font.size.normal}*/px;
    font-style: normal;
    text-decoration: none;
    color: #333333;
  }

hr {
    background-color:#006da6;
    color: #006da6;
}

.content {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : left;
    text-decoration: none;
    width:100%;
    padding-left:15px;
    padding-right:15px;
  }

.content a, .external {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333366;
    text-align : left;
    text-decoration: none;
    background : url('/*{$style.images}*//external.png') no-repeat top right;
    margin-right:1px;
    padding-right:12px;
    padding-left:1px;
    padding-top:0px;
    padding-bottom:1px;
  }

.content a:hover,  .external:hover {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333366;
    text-align : left;
    text-decoration:none;
    background : #F8F8F8 url('/*{$style.images}*//external.png') no-repeat top right;
    border : dotted 1px;
    margin-right:0px;
    padding-left:0px;
    padding-right:12px;
    padding-top:0px;
    padding-bottom:0px;
  }

.content h1 {
    font : bold /*{$font.size.h1}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : left;
    text-decoration: none;
  }

.content h2 {
    font : bold /*{$font.size.h2}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : left;
    text-decoration: none;
  }

.content h3 {
    font : bold /*{$font.size.h3}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : left;
    text-decoration: none;
  }

.content p {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : left;
    text-decoration: none;
    padding-top: 5px;
    padding-bottom: 5px;
  }

.content .anchor {
    display:none;
    background:none;
}

.contimg {
    float : right;
  }

.content table {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : left;
    text-decoration: none;
  }

.content table th {
    border-style : none none none dashed;
    border-width : 1px;
    border-left-color : #006da6;
    padding-left:5px;
    padding-right:10px;
    color : #006da6;
  }

.content table td {
    border-style : dashed none none dashed;
    border-width : 1px;
    border-top-color : #006da6;
    border-left-color : #006da6;
    padding-left:5px;
  }

.content table caption {
    font : bold 14px Verdana, Arial, Helvetica, sans-serif;
    color : #333344;
    text-align : left;
    text-decoration: none;
  }

.infoLinks {
    display: block;
    background : transparent;
    font : normal /*{$font.size.normal}*/px Verdana,Arial,Helvetica,sans-serif;
    color : #333333;
    text-align : left;
    text-decoration : none;
    border-top: 1px solid #ffffff;
    border-bottom: 1px solid #cccccc;
    padding: 0px 0px 2px 5px;
    font-weight: bold;
    overflow: hidden;
    white-space: nowrap;
  }

.infoLinks:hover {
    background : url('/*{$style.images}*//highlight.png') center center;
    font : normal /*{$font.size.normal}*/px Verdana,Arial,Helvetica,sans-serif;
    text-align : left;
    text-decoration : none;
    border-top: 1px solid #cccccc;
    border-bottom: 1px solid #ffffff;
    font-weight: bold;
    display: block;
  }

#backLinks ul {
    padding: 0;
    margin: 0;
  }

#backLinks {
    padding: 0;
    margin: 0;
    list-style-type: none;
  }

#backLinks li ul {
    display: none;
    list-style-type: none;
}

#backLinks:hover li ul {
    display: block;
    list-style-type: none;
}

#backLinks a, #backLinks a:hover {
    text-decoration: none;
    font : normal /*{$font.size.normal}*/px Verdana,Arial,Helvetica,sans-serif;
    font-weight: bold;
    display: block;
  }


.koko5 {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: /*{$font.size.koko5}*/px;
    font-weight: bold;
    color: #333333;
    text-align: right;
  }

.menuBorder {
    background : url('/*{$style.images}*//steelbg-blue.png') repeat-x center center;
    font : bold /*{$font.size.h3}*/px Verdana,Arial,Helvetica,sans-serif;
  }

.newsEvent {
    font : normal /*{$font.size.normal}*/px Verdana,Arial,Helvetica,sans-serif;
    padding : 5px 5px 5px 5px;
    margin : 0 0 15px;
    border-bottom: 1px solid #006da6;
    color : #333333;
    border-left: 1px solid #006da6;
    text-align : left;
    text-decoration : none;
  }

.newsEventTitle {
    background : url('/*{$style.images}*//steelbg-blue.png') repeat-x center center;
    font : bold /*{$font.size.h3}*/px Verdana,Arial,Helvetica,sans-serif;
    color : #333333;
    text-align : left;
    text-decoration : none;
    border-top: 1px solid #00B0F0;
    border-bottom: 1px solid #006da6;
    font-weight: bold;
    padding: 3px;
  }

.newsEventTitle a {
    background : url('/*{$style.images}*//steelbg-blue.png') repeat-x center center;
    font : bold /*{$font.size.h3}*/px Verdana,Arial,Helvetica,sans-serif;
    color : #333333;
    text-align : left;
    text-decoration : none;
    border-top: 1px solid #00B0F0;
    border-bottom: 1px solid #006da6;
    font-weight: bold;
    display:block;
    padding: 3px;
  }

.otsikko {
    font : bold /*{$font.size.koko5}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : right;
  }

.smallTitle {
    font : bold /*{$font.size.h3}*/px Verdana, Arial, Helvetica, sans-serif;
  }

.smallTitle:hover {
    font : bold /*{$font.size.h3}*/px Verdana, Arial, Helvetica, sans-serif;
  }

#infoTable {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    margin : 0 10px;
    color : #333333;
    text-align : center;
  }

.newsEventTitle a:hover {
    background : url('/*{$style.images}*//highlight.png') repeat-x center center;
    font : bold /*{$font.size.h3}*/px Verdana,Arial,Helvetica,sans-serif;
    color : #333333;
    text-align : left;
    text-decoration : none;
    border-top: 1px solid #006da6;
    border-bottom: 1px solid #00B0F0;
    font-weight: bold;
    display:block;
    padding: 3px;
  }

.otsikko:hover {
    font : bold /*{$font.size.koko5}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : right;
    text-decoration: none;
  }

a:hover {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: /*{$font.size.normal}*/px;
    font-style: normal;
    color: #009999;
    text-decoration: none;
  }

#tocinside ul {
    margin:0px;
    padding-top:2px;
    padding-left:15px;
    padding-bottom:0px;
    padding-right:10px;

}

#gbox, .tinted {
    background : url('/*{$style.images}*//steelbg-lightblue.png');
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color: #006da6;
    text-align : left;
    vertical-align : top;
}

#gbox a, #gbox input {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333344;
    text-decoration: none;
  }

#gbox a:hover {
    background : url('/*{$style.images}*//steelbg-blue-hi.png') center center;
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color: #009999;
    text-decoration:none;
  }

.result {
    display: block;
    font : bold /*{$font.size.h3}*/px Verdana, Arial, Helvetica, sans-serif;
    color : #333333;
    text-align : left;
    text-decoration : none;
    border-bottom: 1px solid #00B0F0;
    font-weight: bold;
}

#tocinside li {
    list-style-type: none;
}

.imagebox {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : inherit;
    text-align : left;
    text-decoration: inherit;
    border-style : inherit;
    border-width : inherit;
    border-color : inherit;
    padding-left : 5px;
    float : right;
  }

.imagebox table, .imagebox table td {
    font : normal /*{$font.size.normal}*/px Verdana, Arial, Helvetica, sans-serif;
    color : inherit;
    text-align : left;
    text-decoration: inherit;
    border-style : none;
    border-width : inherit;
    border-color : inherit;
    padding : 0px;
}

